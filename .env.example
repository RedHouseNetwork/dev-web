UID=1000
WEB_ROOT=/home/wobble/web
MYSQL_PASSWORD=changeme
MSSQL_PASSWORD=changeme
POSTGRES_PASSWORD=changeme

# Optional: build PHP containers with SQLCipher (encrypted SQLite).
# Uncomment and set the version to enable per container.
#PHP83_SQLCIPHER_VERSION=4.10.0
#PHP84_SQLCIPHER_VERSION=4.10.0

# Chrome version for Panther E2E tests (set to empty to disable)
#PHP83_CHROME_VERSION=128.0.6613.137
#PHP84_CHROME_VERSION=128.0.6613.137

# Uncomment to install Xdebug (requires rebuild)
#PHP83_XDEBUG=1
#PHP84_XDEBUG=1

# Uncomment to install ffmpeg + yt-dlp (requires rebuild)
#PHP83_FFMPEG=1
#PHP84_FFMPEG=1
